{
  "name": "LGComponents",
  "version": "1.0.0",
  "summary": "Framework containing all the different components the Letgo app uses.",
  "homepage": "https://github.com/letgoapp",
  "license": "Copyright",
  "authors": {
    "letgo team": "ios@letgo.com"
  },
  "source": {
    "git": "https://github.com/letgoapp/letgo-ios.git",
    "tag": "1.0.0"
  },
  "swift_version": "4.0",
  "platforms": {
    "ios": "9.0"
  },
  "static_framework": true,
  "subspecs": [
    {
      "name": "LGLogin",
      "source_files": "LGLogin/LGLogin/Classes/**/*",
      "resource_bundles": {
        "LGLoginBundle": [
          "LGLogin/LGLogin/Assets/**/*"
        ]
      },
      "dependencies": {
        "LGComponents/LGAnalytics": [

        ],
        "LGComponents/LGShared": [

        ],
        "LGComponents/LGResources": [

        ],
        "LGCoreKit": [
          "4.25.0"
        ],
        "FBSDKLoginKit": [
          "4.29.0"
        ],
        "GoogleSignIn": [
          "4.1.1"
        ],
        "RxSwift": [
          "4.0.0"
        ],
        "RxCocoa": [
          "4.0.0"
        ]
      }
    },
    {
      "name": "LGAnalytics",
      "source_files": "LGAnalytics/LGAnalytics/Classes/**/*",
      "dependencies": {
        "Amplitude-iOS": [
          "4.0.4"
        ],
        "AppsFlyerFramework": [
          "4.8.2"
        ],
        "Branch": [
          "0.22.5"
        ],
        "Crashlytics": [
          "3.9.3"
        ],
        "Fabric": [
          "1.7.2"
        ],
        "FBSDKCoreKit": [
          "4.29.0"
        ],
        "Leanplum-iOS-SDK": [
          "2.0.5"
        ],
        "LGCoreKit": [
          "4.25.0"
        ],
        "RxSwift": [
          "4.0.0"
        ]
      }
    },
    {
      "name": "LGShared",
      "source_files": "LGShared/LGShared/Classes/**/*",
      "frameworks": "CoreText",
      "dependencies": {
        "AlamofireImage": [
          "3.3.0"
        ],
        "SwiftyUserDefaults": [
          "3.0.1"
        ],
        "CocoaLumberjack/Swift": [
          "3.3.0"
        ]
      }
    },
    {
      "name": "LGResources",
      "source_files": "LGResources/LGResources/Classes/**/*",
      "resource_bundles": {
        "LGResourcesBundle": [
          "LGResources/LGResources/Assets/**/*"
        ]
      },
      "script_phases": {
        "name": "Generate R structure",
        "script": "${PODS_TARGET_SRCROOT}/LGResources/LGResources/generate-r-struct.sh",
        "execution_position": "before_compile"
      }
    }
  ]
}
